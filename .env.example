# Google Cloud Platform
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=./credentials/service-account.json

# Airflow
AIRFLOW_UID=50000
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# BigQuery
BQ_DATASET_RAW=raw
BQ_DATASET_STAGING=staging
BQ_DATASET_WAREHOUSE=warehouse
BQ_DATASET_FEATURES=features

# Cloud Storage
GCS_BUCKET_RAW=your-project-id-raw-data
GCS_BUCKET_STAGING=your-project-id-staging-data
GCS_BUCKET_PROCESSED=your-project-id-processed-data

# API
API_KEY=your-api-key-here
API_HOST=0.0.0.0
API_PORT=8000

# Data Generation
DATA_OUTPUT_DIR=./data/generated
DEFAULT_PACKAGE_COUNT=10000

# Logging
LOG_LEVEL=INFO
